<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../components/head %>
</head>
<body>
    <main>
        <% if(json) { %>
        <h1>
            You found the Device Lab! Good Job!
        </h1>
        <a href="/">
            Stop the navigation
        </a>
        <ul>
            <li>
                <a href="/start" class="completed">Start</a>
            </li>
            <% json.steps.forEach(function (step, i) { %>
                <li>
                    <a href="#<%= i + 1 %>"><%= i + 1 %></a>
                </li>
            <% }) %>
            <li>
                <a href="/finish" class="current">Finish</a>
            </li>
        </ul>
        <% } %>
    </main>
</body>
</html>